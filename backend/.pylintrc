[MASTER]
init-hook="from pylint.config import find_pylintrc; import sys; sys.path.append(os.path.dirname(find_pylintrc()))"

[MESSAGES CONTROL]
max-args=7
disable=
  duplicate-code,
  fixme,
  invalid-name,
  missing-docstring,
  too-few-public-methods,
  too-many-locals,
  too-many-return-statements
